{
  "name": "equipment-management-system",
  "version": "1.0.0",
  "description": "Equipment Management System - A modern full-stack web application for managing equipment, maintenance, requests, and reports",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\"",
    "build": "npm run client:build",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm run dev",
    "client:start": "cd client && npm run preview",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && npm run install:server && npm run install:client",
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm run test"
  },
  "keywords": [
    "equipment-management",
    "react",
    "typescript",
    "nodejs",
    "express",
    "sql-server",
    "vite",
    "tailwindcss",
    "shadcn-ui"
  ],
  "author": "duongloc216",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duongloc216/Management-Equipment.git"
  },
  "bugs": {
    "url": "https://github.com/duongloc216/Management-Equipment/issues"
  },
  "homepage": "https://github.com/duongloc216/Management-Equipment#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
